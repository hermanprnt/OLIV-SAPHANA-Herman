<div id="lookupeFakturPopupSearch" class="modal fade">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"  onclick="closePopUpEFaktur()">
                    <span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="popup-title">
                    E-Faktur Lookup</h4>
            </div>
            <div class="modal-body">
                @*<div class="row ">
                    <div class="col-xs-12 col-md-12">*@
                <div class="form-group form-group-xs">
                    <div>
                        <div class="input-group input-group-xs">
                            <input id="txtVendorCodeEfaktur" class="form-control" type="hidden"/>
                            <input id="txtSearchLookupEfaktur" class="form-control" type="text" placeholder="input tax invoice no...."
                                onkeypress="onEnterSearchDataLookupEfaktur(event);" />
                            <span class="input-group-btn">
                                <button type="button" id="btnSearchLookupEfaktur" data-loading-text="<i class='glyphicon-refresh-animate glyphicon glyphicon-refresh'></i>"
                                    class=" btn btn-default btn-sm" style="line-height: 1.1; top: -1px; left: 5px;" onclick="javascript:onValidateEfakturLookup()">
                                    &nbsp<i class="fa fa-search"></i>
                                </button>
                                <button id="btnClearSearchLookupEfaktur" class="btn btn-default btn-sm" title="Clear" type="button"
                                    style="line-height: 1.1; top: -1px; left: 10px; width: 43px;" onclick="javascript:clearPopupFilterEfaktur()">
                                        &nbsp<i class="fa  fa-eraser"></i>&nbsp
                                </button>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="form-group form-group-xs">
                    @Html.Partial("_LookupTableEFaktur")
                </div>
                @*</div>
                </div>*@
            </div>
            <div class="modal-footer">
                <button type="button" id="btncloselookup" class="btn btn-default btn-sm" onclick="closePopUpEFaktur()">
                    close</button>
            </div>
        </div>
    </div>
</div>
