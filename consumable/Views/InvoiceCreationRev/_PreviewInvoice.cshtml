    function validateInputForm() {
        var valid = true;

        if ($("#invoiceNo").val() == '') {
            $("#invoiceNo").css("background-color", "#F2BCD5");
            valid = false;
        }
        else {
            $("#invoiceNo").css("background-color", "#FFFFFF");
