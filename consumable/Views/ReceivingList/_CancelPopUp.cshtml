<div id="cancelpopup" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">&times;</span> <span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="popup-title">
                    Cancel GR
                </h4>
            </div>
            <div class="modal-body">
                <form role="form" class="form-horizontal _criteria" id="cancelForm">
  
                <input type="hidden" id="matDocNumber" />
                <input type="hidden" id="entrySheetNum" />
                <input type="hidden" id="matDocYear" />
                <input type="hidden" id="matDocDocdate" />

                <div class="row">
                    <div class="col-xs-12 col-sm-6 col-md-9">
                        @* Date *@
                        <div class="form-group form-group-xs">
                            <label class="col-xs-4 col-sm-3 col-md-5 control-label text-muted">
                                Date</label>
                            <div class="col-xs-8 col-sm-9 col-md-5">
                                <div class="input-group" style="width: 100%; cursor: pointer;">
                                    <input class="form-control date-picker mandatory" id="matDocPostDate" type="text" data-date-format="dd.mm.yyyy" />
                                    <span class="input-group-addon"><i class="fa fa-calendar bigger-110"></i></span>
                                </div>
                            </div>
                        </div>
                        @* Remarks *@
                        <div class="form-group form-group-xs">
                            <label class="col-xs-4 col-sm-3 col-md-5 control-label text-muted">
                                Remarks
                            </label>
                            <div class="col-xs-8 col-sm-9 col-md-5">
                                <div class="input-group" style="width: 100%;">
                                    <input id="matDocText" class="form-control mandatory" type="text" value="" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row" style="text-align: right; padding-right: 0px; padding-top: 5px;
                    margin-top: 10px; margin-right: 2px;">
                    <button type="button" class="btn btn-primary btn-xs" style="width: 60px;"
                        onclick="javascript:confirmCancelGR()">
                        Submit</button>
                    <button type="reset" class="btn btn-default btn-xs" style="width: 60px;">
                        Clear</button>
                    <button class="btn btn-danger btn-xs" data-dismiss="modal" style="width: 60px;">
                        Close</button>
                </div>
                </form>
            </div>
        </div>
        @*<div class="modal-footer">
            <button type="button" id="btncloselookup" class="btn btn-default btn-xs" data-dismiss="modal">
                close</button>
        </div>*@
    </div>
</div>

