<div id="tblSupplierPopupInput" class="table-responsive">
    <table class="table table-striped table-bordered table-condensed">
        <thead>
            <tr>
                <th class="text-center">
                    Supplier Code
                </th>
                <th class="text-center">
                    Supplier Name
                </th>
            </tr>
        </thead>
        @foreach (var item in (List<consumable.Models.SUPPLIER.Supplier>)ViewData["Suppliers"])
        {
            <tr>
                <td class="text-center cursor-link" onclick="javascript:onLookupSelectedInput('@item.SUPPLIER_CD','@item.SUPPLIER_NAME')">@item.SUPPLIER_CD
                </td>
                <td class="text-center cursor-link" onclick="javascript:onLookupSelectedInput('@item.SUPPLIER_CD','@item.SUPPLIER_NAME')">@item.SUPPLIER_NAME
                </td>
            </tr>
        }
    </table>
    <div class="col-xs-6" style="margin-top: 5px;"></div>
    @Html.Partial("_UIPagingLookup")
    <script type="text/javascript" language="javascript">
        $(".IndexPagingLookup").click(function () {
            var pg = $(this).attr('Page');
            //alert(pg);
            onLookupSupplierInput(pg);
        });
    </script>
</div>
