<form role="form" class="form-horizontal _criteria" id="searchForm">
<div class="row">
    <div class="col-xs-12 col-sm-6 col-md-6">
        <div class="form-group form-group-xs _criteria">
            <label class="col-xs-4 col-sm-3 col-md-4 control-label text-muted">
                Supplier Code
            </label>
            <div class="col-xs-8 col-sm-8 col-md-8">
                <input type="text" id="supplierCodeSearch" class="form-control" />
            </div>
        </div>
        <div class="form-group form-group-xs _criteria">
            <label class="col-xs-4 col-sm-4 col-md-4 control-label text-muted">
                Supplier Name</label>
            <div class="col-xs-8 col-sm-8 col-md-8">
                <input type="text" id="supplierNameSearch" class="form-control" />
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-6">
        <div class="form-group form-group-xs _criteria">
            <label class="col-xs-4 col-sm-3 col-md-4 control-label text-muted">
                Approver Unit Code
            </label>
            <div class="col-xs-8 col-sm-9 col-md-7">
                <div class="input-group pos-rel" style="width: 100%;">
                    <input id="approverUnitCodeSearch" class="form-control" type="text" value="" />
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-12 col-sm-6 col-md-4">
        <div class="form-group form-group-xs _criteria">
            
        </div>
    </div>
</div>
<div class="row" style="margin-bottom: 10px;">
    <div class="col-xs-6" style="margin-top: 5px;">
    </div>
    <div class="col-xs-6 text-right">
        <div class="form-group-xs _criteria">
            <button type="button" id="btnSearch" class="btn btn-primary btn-xs" onclick="javascript:searchData(1)">
                Search
            </button>
            <button type="reset" class="btn btn-default btn-xs">
                Clear
            </button>
        </div>
    </div>
</div>
</form>
