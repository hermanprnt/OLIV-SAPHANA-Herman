					wbsNo: $('#wbsNoSearch').val(),
					prNo: $('#prNoSearch').val(),
                    page: page,
                    size: $("#pageSize").val()			
				},
				beforeSend:function () {
                    $('#btnSearch').button('loading');
                },
				success: function (data) {
					$("#tablegrid").html(data);	
				},
				complete:function () {
                    $('#btnSearch').button('reset');
                },
                error: function(data) {
                   $('#btnSearch').button('reset');
                }
			});
			
		}

        function onEnterSearchData(e) {
            var key = e.keyCode || e.which;
            if (key == 13) {
                searchData(1);
            }
