<div id="tblSupplierPopupSearch" class="table-responsive multiselect-item filter" style="overflow: hidden;">
    <div style="border: 1pt solid #ccc;">
        <table class="table table-striped table-bordered table-condensed table-fixed">
            <thead>
                <tr>
                    <th class="text-center" style="width: 14px; max-width: 14px">
                        <input class="gridLookup-checkbox cursor-link" type="checkbox" style="height: 13px;
                            margin-top: 3px;" id="gridLookup-checkAll" />
                    </th>
                    <th class="text-center" style="width: 100px; max-width: 100px">
                        Supplier Code
                    </th>
                    <th class="text-center" style="width: 198px; max-width: 198px">
                        Supplier Name
                    </th>
                    <th class="text-center" style="width: 9px; max-width: 9px">
                    </th>
                </tr>
            </thead>
        </table>
    </div>
    <div style="overflow: scroll; height: 153px;">
        <table class="table table-striped table-bordered table-condensed table-fixed">
            @foreach (var item in (List<consumable.Models.SUPPLIER.Supplier>)ViewData["Suppliers"])
            {
                <tr>
                    @*<td class="text-center cursor-link" onclick="javascript:onGridLookupSelected('Data-@i')">Data-@i</td>*@
                    <td class="text-center" style="width: 20px; max-width: 20px">
                        <input type="checkbox" class="gridLookup-checkbox gridLookup-checkbox-body cursor-link" 
                            style="height: 13px; margin-top: 3px;" data-value="@item.SUPPLIER_CD" />
                    </td>
                    <td class="text-center" style="width: 135px; max-width: 135px">
                        @item.SUPPLIER_CD
                    </td>
                    <td class="text-left" style="width: 268px; max-width: 268px">
                        @item.SUPPLIER_NAME
                    </td>
                </tr>
            }
        </table>
    </div>
    <div>
        <table class="table table-striped table-bordered table-condensed table-fixed" style="width: 100%">
            <tfoot>
                <tr>
                    <td colspan="13">
                        @Html.Partial("_UIPagingLookup")
                        <script type="text/javascript" language="javascript">
                            $(".IndexPagingLookup").click(function () {
                                var pg = $(this).attr('Page');
                                //alert(pg);
                                onLookupSupplier(pg);
                            });
                        </script>
                    </td>
                </tr>
            </tfoot>
        </table>
    </div>
</div>